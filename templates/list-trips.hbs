{{#each this}}
  <div>
    {{location}}
  </div>
    {{#each reviews}}
      <div id="{{@key}}">
        <span>{{title}}</span> | <span>{{date}}</span>
        {{!-- <p>{{text}}</p><span id="{{@index}}" class="glyphicon glyphicon-trash" aria-hidden="true"></span> --}}
      </div>
    {{/each}}
    <button id="add-review#{{@key}}" type="button">Add Review</button>
{{/each}}

